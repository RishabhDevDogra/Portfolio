          <ul class="navbar-nav">

            <li class="nav-item">
              <% if (title == "Home") { %>
                  <a class="nav-link active" href="/homepage">
              <% } else { %>
                  <a class="nav-link" href="/homepage">
              <% } %>            
                  <i class="fas fa-home"></i> HOME</a>
              </a>
            </li>
            <li class="nav-item">
