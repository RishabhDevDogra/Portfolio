                    <i class="fas fa-shield-alt"></i> service </a>
                </a>               
                <li class="nav-item">
                  <% if (title == "aboutme") { %>
                      <a class="nav-link active" href="/aboutme">
                  <% } else { %>
                      <a class="nav-link" href="/aboutme">
                  <% } %>            
                      <i class="fas fa-user"></i> About</a>
                  </a>        
                  </li>
                  </li>
                  <li class="nav-item">
                    <% if (title == "contact") { %>
                        <a class="nav-link active" href="/contact">
                    <% } else { %>
                        <a class="nav-link" href="/contact">
                    <% } %>            
                        <i class="fas fa-search"></i> Contact </a>
                    </a>           
            
          </ul>
